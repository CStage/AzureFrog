{  
    "manifest_version": 3,  
    "name": "Azure Frog",  
    "description": "This extension allows you to quickly copy a pretty link from Azure DevOps",  
    "version": "1.2",  
    "action": {  
        "default_icon": "Images/AzureFrog.png",  
        "default_popup": "popup.html"  
    },  
    "permissions": ["activeTab", "tabs", "webNavigation", "scripting"]  
}  